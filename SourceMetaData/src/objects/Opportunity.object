    <validationRules>
        <fullName>Only_enter_feedback_on_closed_oppertunit</fullName>
        <active>true</active>
        <description>Can only add data to feedback field when an opportunity is closed</description>
        <errorConditionFormula>ISCHANGED( Customer_Feedback__c ) &amp;&amp; 
NOT(
 CONTAINS(TEXT(StageName) , &quot;Closed&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Customer_Feedback__c</errorDisplayField>
        <errorMessage>Sorry you are only allowed to add customer feedback onto an opportunity with a closed stage. Please mark opportunity as closed and then complete this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Probability_cannot_be_zero</fullName>
        <active>true</active>
        <description>Cannot leave the Probability at zero</description>
        <errorConditionFormula>( Probability = 0 ) &amp;&amp;
NOT( ISPICKVAL( StageName , &quot;Closed Lost&quot;) )</errorConditionFormula>
        <errorMessage>Please enter a non-zero value for the Probability of Sale</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_cannot_be_after_the_End_Date</fullName>
        <active>true</active>
        <description>Start Date cannot be after the End Date</description>
        <errorConditionFormula>Start_Date__c &gt;  End_Date__c</errorConditionFormula>
        <errorMessage>The Start Date cannot be after the End Date</errorMessage>
    </validationRules>
