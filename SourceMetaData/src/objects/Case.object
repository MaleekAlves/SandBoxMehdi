    <fields>
        <fullName>Case_Summary__c</fullName>
        <description>Formula field which merges the non blank picklist and free text fields used to describe the case to provide a single, 1 line summary of the case.</description>
        <externalId>false</externalId>
        <formula>CASE(
    $RecordType.Name,
    
    &quot;CSC&quot;,
    TEXT(Service_Area__c) +  IF(!ISBLANK(TEXT(CSC_Case_Type__c)),&quot; - &quot; + TEXT(CSC_Case_Type__c)+&quot;.&quot;,&quot;&quot;) +  IF(!ISBLANK( Goal_Free_Text__c ),&quot; &quot; + Goal_Free_Text__c,&quot;&quot;),  
    
    &quot;CCC&quot;, 
    IF (
	OR( ISBLANK(TEXT(Popular_Goals__c)),  CONTAINS(TEXT(Popular_Goals__c), &quot;Other&quot;) ) , 
        IF (
       	    ISBLANK(TEXT(Goal_Type__c )) ,
            IF (
                ISBLANK(Goal_Free_Text__c) , 
                IF(!ISBLANK(SuppliedEmail), &quot;&quot;, &quot;Unspecified&quot;),
                Goal_Free_Text__c 
            ), 
            TEXT(Goal_Type__c ) + IF(!ISBLANK(TEXT(Goal_Sub_Type__c)),&quot; - &quot; + TEXT(Goal_Sub_Type__c),&quot;&quot;)   
        ),       
        TEXT(Popular_Goals__c)
    ),

    &quot;Error&quot;
)</formula>
        <inlineHelpText>Formual field which merges the non blank picklist and free text fields used to describe the case to provide a single, 1 line summary of the case.</inlineHelpText>
        <label>Summary</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
