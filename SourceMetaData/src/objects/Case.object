    <validationRules>
        <fullName>Calendar_Days_Elapsed_only_Increased</fullName>
        <active>false</active>
        <description>Users with Permission Set LBH Support Manager can change number of Calendar Days Elapsed, but only to increase it</description>
        <errorConditionFormula>IF(SLA__r.Working_Days__c = TRUE &amp;&amp; 
IsReopenGoal__c = FALSE &amp;&amp; 
NOT (ISCHANGED( Active_SLA__c )) &amp;&amp; 
NOT (ISCHANGED( Continuous_Working_Days_on_Hold__c )) &amp;&amp; 
NOT (ISCHANGED( Status)) , 

Calendar_Days_Elapsed__c &lt;= PRIORVALUE( Calendar_Days_Elapsed__c ),
FALSE)</errorConditionFormula>
        <errorDisplayField>Calendar_Days_Elapsed__c</errorDisplayField>
        <errorMessage>Sorry, number of calendar days elapsed can only be increased</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Must_have_a_summary_type</fullName>
        <active>true</active>
        <description>A case must have a type, or other summary or otherwise be a new draft blank email</description>
        <errorConditionFormula>AND(
$RecordType.Name &lt;&gt; &quot;CSC&quot;,
ISBLANK(TEXT(Goal_Type__c)),
ISBLANK(TEXT(Goal_Sub_Type__c)),
ISBLANK(TEXT(Popular_Goals__c)),
ISBLANK(Goal_Free_Text__c),
ISBLANK(SuppliedEmail)
)</errorConditionFormula>
        <errorDisplayField>Popular_Goals__c</errorDisplayField>
        <errorMessage>Please select what the goal is about</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cases_Cant_have_Parents</fullName>
        <active>true</active>
        <description>A Goal cannot have a parent</description>
        <errorConditionFormula>!ISBLANK(ParentId)</errorConditionFormula>
        <errorMessage>A goal cannot have a parent case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cases_Must_have_contact_or_anonymous</fullName>
        <active>true</active>
        <description>A case must either have a selected contact, or marked as anonymous</description>
        <errorConditionFormula>(!ISBLANK(ContactId) &amp;&amp; Anonymous_Customer__c == true)</errorConditionFormula>
        <errorDisplayField>ContactId</errorDisplayField>
        <errorMessage>A goal must either have a selected contact, or marked as anonymous</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpenedDate_not_in_Future</fullName>
        <active>true</active>
        <description>Opened Date can not be the future date</description>
        <errorConditionFormula>Opened_Date__c &gt; TODAY ()</errorConditionFormula>
        <errorMessage>Please set Opened Date as today or earlier. Opened Date can not be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_not_reversed_to_Draft</fullName>
        <active>true</active>
        <description>Goal status can not be reversed to be in Draft status once it has been Opened.</description>
        <errorConditionFormula>AND(ISCHANGED(Status), ISPICKVAL(Status, &quot;Draft&quot;))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Status can not be reversed to Draft</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Working_Days_Elapsed_Only_Increased</fullName>
        <active>false</active>
        <description>Users with Permission Set LBH Support Manager can change number of Working Days Elapsed, but only to increase it</description>
